{
	"manifest_version": 2,
	"name": "Now",
	"description": "A simple New Tab page that displays the time and date.",
	"version": "1.0.2",
	"permissions": ["storage"],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"chrome_url_overrides": {
		"newtab": "now.html"
	}
}